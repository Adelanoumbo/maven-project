pipeline {
    agent none
    stages {


        stage('maven') {
              agent {
        docker { image 'maven:3.6.0-jdk-11-slim' }
    }
            steps {
                sh '''
                mvn clean install package
                '''
            }
        }


         stage('git') {
              agent {
        docker { image 'kemvoueric/ubuntu_java_maven_git:latest' }
    }
            steps {
                sh '''
               git clone https://github.com/kemvoueric/sample-project-maven.git
                '''
            }
        }

    }
}
