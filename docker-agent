pipeline {
    agent none
    stages {


        stage('maven') {
              agent {
        docker { image 'maven:3.6.0-jdk-11-slim' }
    }
            steps {
                sh '''
                mvn clean install package
                '''
            }
        }


         stage('git') {
              agent {
        docker { image 'alpine/git' }
    }
            steps {
                sh '''
               git branch
                '''
            }
        }

    }
}
